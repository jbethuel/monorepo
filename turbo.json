{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "build": {
      "dependsOn": ["^lint"],
      "outputs": ["build/**"]
    },
    "@monorepo/ui#build": {
      "dependsOn": ["^lint"],
      "outputs": ["build/**"]
    },
    "@monorepo/web#build": {
      "dependsOn": ["^lint", "@monorepo/ui#build"],
      "outputs": ["build/**"]
    }
  }
}
